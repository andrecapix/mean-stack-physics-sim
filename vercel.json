{
  "version": 2,
  "name": "mean-stack-physics-sim",
  "builds": [
    {
      "src": "mean-ui/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/mean-ui",
        "framework": "angular"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}